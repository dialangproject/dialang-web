<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html lang="{{al}}">
    <head>
        <meta http-equiv="content-type" content="text/html; charset=utf-8">
        <meta name="google" value="notranslate">
        <link rel="stylesheet" type="text/css" href="/css/dialang.css" media="all"/>
        <link rel="stylesheet" type="text/css" href="/css/smoothness/jquery-ui-1.9.2.custom.min.css" media="all"/>
        <script type="text/javascript" src="/lib/jquery-1.8.3.js"></script>
        <script type="text/javascript" src="/lib/jquery-ui-1.9.2.custom.min.js"></script>
        <script type="text/javascript" src="/lib/jquery.form.js"></script>
        <script type="text/javascript" src="/lib/mustache.js"></script>
        <script type="text/javascript">
            var dialang = {
              state: '{{state}}',
              flags: {
                disallowInstantFeedback: ('{{disallowInstantFeedback}}' === 'true') ? true : false,
                hideALS: ('{{hideALS}}' == 'true') ? true : false,
                hideTLS: ('{{hideTLS}}' == 'true') ? true : false,
                hideVSPT: ('{{hideVSPT}}' == 'true') ? true : false,
                hideVSPTResult: ('{{hideVSPTResult}}' == 'true') ? true : false,
                hideSA: ('{{hideSA}}' == 'true') ? true : false,
                hideTest: ('{{hideTest}}' == 'true') ? true : false,
                hideFeedbackMenu: ('{{hideFeedbackMenu}}' == 'true') ? true : false
              },
              session: {
                  al: '{{al}}',
                  tl: '{{tl}}',
                  skill: '{{skill}}',
                  vsptDone: {}
              },
              pass: {
                  baskets: [],
                  itemToBasketMap: {},
                  items: [],
                  subskills: {}
              }
            };
        </script>
    </head>
    <body>
        <div id="container">
            <div id="topbar">
                <div id="navbar">
                    <button id="skipback"><img src="/images/skipb.gif"/></button>
                    <button id="back"><img src="/images/prev.gif"/></button>
                    <button id="next"><img src="/images/next.gif"/></button>
                    <button id="skipforward"><img src="/images/skipf.gif"/></button>
                    <button id="help"><img src="/images/help.gif" height="23px;"/></button>
                    <button id="instantfeedback"><img src="/images/instantFeedbackOff.gif" height="23px;"/></button>
                    <button id="keyboard-button"><img src="/images/keyb.gif" height="23px;"/></button>
                    <button id="save-button"><img src="/images/savetodrive.png" height="23px;" disabled="disabled"/></button>
                </div>  <!-- /navbar -->
                <div id="logo">
                    <div id="progressbar" class="dialang-progressbar"><div id="progressbar-label"></div></div>
                    <img src="/images/Logo.gif"/>
                </div> <!-- /logo -->
            </div> <!-- /topbar -->

            <div id="help-dialog">
            </div> <!-- /help-dialog -->

            <div id="save-dialog">
            </div> <!-- /save-dialog -->

            <div id="load-dialog">
            </div> <!-- /load-dialog -->

            <div id="content">
            </div> <!-- /content -->
            <div class="bottom-margin"></div>
        </div> <!-- /container -->
        <div id="keyboard-dialog">
            <div id="keyboard"></div>
        </div>
        <script type="text/javascript" src="/js/dialang.js"></script>
        <script type="text/javascript" src="/js/dialang_utils.js"></script>
        <script type="text/javascript" src="/js/navigation.js"></script>
    </body>
</html>
