<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
"http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app>

    <resource-ref>
        <description>Connection Pool</description>
        <res-ref-name>jdbc/dialang</res-ref-name>
        <res-type>javax.sql.Datasource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>

    <servlet>
        <servlet-name>scorevspt</servlet-name>
        <servlet-class>org.dialang.servlets.ScoreVSPT</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>settls</servlet-name>
        <servlet-class>org.dialang.servlets.SetTLS</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>recordsa</servlet-name>
        <servlet-class>org.dialang.servlets.RecordSA</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>starttest</servlet-name>
        <servlet-class>org.dialang.servlets.StartTest</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>scorevspt</servlet-name>
        <url-pattern>/scorevspt</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>settls</servlet-name>
        <url-pattern>/settls</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>recordsa</servlet-name>
        <url-pattern>/recordsa</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>starttest</servlet-name>
        <url-pattern>/starttest</url-pattern>
    </servlet-mapping>

</web-app>
